{
	"compilerOptions": {
		"baseUrl": ".",
		"module": "commonjs",
		"target": "es6",
		"jsx": "preserve",
		"lib": ["dom", "es2015", "es2017"],
		"strict": true,
		"noEmit": true,
		"isolatedModules": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"removeComments": false,
		"paths": {
			"components": ["./src/components"],
			"components/*": ["./src/components/*"],
			"gatsby-config": ["./gatsby-config.js"],
			"styles/*": ["./src/styles/*"],
			"svg/*": ["./src/svg/*"],
			"utils/*": ["./src/utils/*"]
		}
	},
	"include": ["./src"]
}
